<header>
  <h1>Projects</h1><small>Sign in with admin@ticketee.com & 'password' to poke around.
    <% admins_only do %>
      <ul class="actions">
        <li>
          <%= link_to "New Project", new_admin_project_path, class: "new" %>
        </li>
      </ul>
    <% end %>
</header>

<div id="projects">
  <% @projects.each do |project| %>
    <h2><%= link_to project.name, project %></h2>
    <p><%= project.description %></p>
  <% end %>
</div>
